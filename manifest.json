{
  "manifest_version": 3,
  "name": "图片翻译助手",
  "version": "1.0.0",
  "description": "右键点击图片快速使用 Google Lens 翻译",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "service_worker": "src/background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
